		</userTask>
		<endEvent id="end" name="End" shareniu:id="shareniuId"
			shareniu:name="shareniuName">
		</endEvent>
		<sequenceFlow id="flow2" sourceRef="operationTask"
			targetRef="end"></sequenceFlow>
